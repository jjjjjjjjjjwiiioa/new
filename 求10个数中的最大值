//用数组内的所有数 作比较 谁大 谁赋值给max

#include<stdio.h>
#include<string.h>
int main()
{
    int arr[]={1,2,3,4,5,45,7,8,9,10};
    int max= 0;
    int i= 0;
    int sz= sizeof(arr)/sizeof(arr[0]);
    for(i= 0;i<sz; i++)
    {
        if(arr[i]> max)
        {
            max= arr[i];
        }
    }
    printf("%d", max);
    return 0;
}
